{
  "name": "server",
  "scripts": {
    "prod": "dotenv -- ts-node src/index.ts",
    "start": "dotenv -- nodemon -e ts,graphql --ignore src/generated/schema.graphql --ignore src/resolver.types.ts -x ts-node src/index.ts",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "build": "rimraf dist && tsc",
    "prisma-deploy": "ts-node prisma/datamodel.ts && prisma deploy",
    "prisma-generate": "ts-node prisma/datamodel.ts && prisma generate"
  },
  "dependencies": {
    "@types/lodash": "^4.14.135",
    "@types/module-alias": "^2.0.0",
    "bcryptjs": "2.4.3",
    "fs": "^0.0.1-security",
    "graphql-shield": "^5.3.5",
    "graphql-tools": "^4.0.4",
    "graphql-yoga": "^1.18.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "mkdirp-sync": "^0.0.3",
    "module-alias": "^2.2.0",
    "nexus": "^0.12.0-beta.6",
    "nexus-prisma": "^0.3.7",
    "prisma-binding": "^2.3.11",
    "prisma-client": "^0.0.0",
    "prisma-client-lib": "^1.34.0",
    "shortid": "^2.2.14",
    "slugify": "^1.3.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/node": "10.14.7",
    "dotenv-cli": "1.4.0",
    "graphql-cli": "^3.0.11",
    "merge-graphql-schemas": "^1.5.8",
    "nodemon": "1.19.1",
    "prisma": "1.33.0",
    "rimraf": "2.6.3",
    "ts-node": "7.0.1",
    "typescript": "3.2.2"
  }
}
